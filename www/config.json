{
  "dataPath": "/",
  "siteName": "Karte Freifunk Berlin",
  "mapSigmaScale": 0.5,
  "showContact": true,
  "maxAge": 14,
  "internal_comment": "compare maps on http://mc.bbbike.org/",
  "mapLayers": [
    {
      "name": "OSM SE hydda roads",
      "url": "http://{s}.tile.openstreetmap.se/hydda/roads_and_labels/{z}/{x}/{y}.png",
      "config": {
        "#subdomains": "1234",
        "type": "osm",
        "attribution": "Tiles &copy; 2016 <a href=\"http://bbbike.org/\">BBBike.org</a> &amp; <a href=\"https://www.geofabrik.de/\">Geofabrik GmbH</a> - map data (&copy;) <a href=\"https://www.openstreetmap.org/\">OpenStreetMap.org</a>",
        "maxZoom": 18
      }
    },
    {
      "name": "OSM Mapnik (DE)",
      "url": "http://{s}.tile.openstreetmap.de/tiles/osmde/{z}/{x}/{y}.png",
      "config": {
        "#subdomains": "1234",
        "type": "osm",
        "attribution": "Tiles &copy; 2016 <a href=\"http://bbbike.org/\">BBBike.org</a> &amp; <a href=\"https://www.geofabrik.de/\">Geofabrik GmbH</a> - map data (&copy;) <a href=\"https://www.openstreetmap.org/\">OpenStreetMap.org</a>",
        "maxZoom": 18
      }
    }
  ],
  "nodeInfos": [
    {
      "name" : "OpenWifiMap",
      "href" : "https://util.berlin.freifunk.net/knoteninfo?knoten={NODE_ID}&typ=owm",
      "caption" : "{NODE_ID} auf openwifimap.net"
    },
    {
      "name" : "monitor.berlin.freifunk.net",
      "href" : "https://util.berlin.freifunk.net/knoteninfo?knoten={NODE_ID}&typ=monitor",
      "caption" : "{NODE_ID} auf monitor.berlin.freifunk.net"
    },
    {
      "name" : "wiki.freifunk.net/Berlin",
      "href" : "https://util.berlin.freifunk.net/knoteninfo?knoten={NODE_ID}&typ=wiki",
      "caption" : "{NODE_ID} auf den Berliner Seiten auf wiki.freifunk.net"
    }
  ]
}
